---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>notes on camp</title>
  <link rel="alternate" type="text/html" href="http://notesoncamp.com/" />
  <link rel="self" type="application/atom+xml" href="http://notesoncamp.com/atom.xml" />
  <id>http://notesoncamp.com/atom.xml</id>
  <updated>{{ site.time | date_to_utc | date:"%Y-%m-%dT%H:%M:%SZ" }}</updated>
  <subtitle>this blog was born under the sign of saturn</subtitle>

  {% for post in site.posts limit:15 %}
  
  <entry>
    <title>{{ post.title }}</title>
    <link rel='alternate' type='text/html' href='{{ post.url }}' />
    <id>tag:metajack.im:{% if forloop.index0 > last_dash_index %}{{ post.id | replace:"-","_" }}{% else %}{{ post.id }}{% endif %}</id>
    <updated>{{ post.date | date.getutc:"%Y-%m-%dT%H:%M:00Z" }}</updated>
    <updated></updated>

    <author>
      <name>Theodore O. Nielsen</name>
      <uri>http://notesoncamp.com/</uri>
      <email>nielsen@notesoncamp.com</email>
    </author>

    <summary><![CDATA[{{ post.content ]]></summary>
    <content type='html' xml:lang='en' xml:base='http://notesoncamp.com/'>
      <![CDATA[{{ post.content }}{{ post.extended }}]]>
    </content>
  </entry>
  {% endfor %}
</feed>